# arithmetic opratior   
a = 5                     
b = 2                     
print (a+b)   
print (a-b)
print (a*b)
print (a/b)
print (a**b)
print (a%b)

# relatinol opratiros 
a=50 
b=20
print (a==b)
print (a!=b)
print (a<=b)
print (a>=b)

# assignment opratior 
num = 10 
num = num+10
